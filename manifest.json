{
    "manifest_version": 3,
    "name": "BHSE",
    "description": "Search your browser history as if it were your own private search engine",
    "version": "0.0.1",
    "action": {
        "default_popup": "search.html",
        "default_icon": "bhse.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}